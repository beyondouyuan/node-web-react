{
    "presets": ["es2015", "react", "stage-1"],
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
                "style": "css"
            },
            "react-transform", {
                "transforms": [{
                    "transform": "react-transform-hmr",

                    "import": ["react"],
                    "locals": ["modules"]
                }]
            }
        ]
    ]
}
